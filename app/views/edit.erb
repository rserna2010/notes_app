<div class="container">
  <h1>Edit Note: <%=@note.title %></h1>
  <p><strong></strong></p>


    <form action="/notes/note.id" method='post'>
    <input type="hidden" name="_method" value="put">
    <table id="login_form">
    <tr>
    <td>Title:</td>
    <td><input type="text" name="title" value="<%=@note.title%>"></td/></tr>
    <tr>
    <td>Content:</td>
    <td><textarea id="paragraph" rows="5" cols="40" name="content"><%=@note.content%></textarea></td></tr>
    <tr>
    </table><br>
    <input type="Submit" value="Save Note!">
    </form>  
